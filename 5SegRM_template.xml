<mujoco model="3SegRMdevel">
	<compiler coordinate="local" angle="radian" inertiafromgeom="false" inertiagrouprange="0 10"/>
	<option timestep="0.001" integrator="RK4" collision="predefined">
		<!-- <flag gravity="enable" passive="disable"/> -->

	</option>
	<size nkey="1" />

	<default>
        <geom rgba=".8 .6 .4 0.5" solimp=".99 .99 0"/>
        <joint limited="true" range="-1.57 0"/><!-- relative to reference pose-->
	</default>

	<asset>
		<texture type="skybox" builtin="gradient" rgb1="1 1 1" rgb2=".6 .8 1" width="256" height="256"/>
		<texture name="texplane" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2" 
            width="100" height="100"/>  
		<material name='MatPlane' reflectance='0.5' texture="texplane" texrepeat="1 1" texuniform="true"/>

	</asset>
<!-- cumulative lengths 0.01, 0.0180277, 0.0384948, 0.056286, 0.0760299 -->
	<worldbody><!-- BODY 0 -->
		<light pos="0 1 1" dir="0 -1 -1" diffuse="1 1 1"/>
		<geom name='floor' pos='0 0 -0.002' size='1 1 0.125' type='plane' material="MatPlane"/><!-- make the floor the same distance as the foot sphere radius -->
		<body name="m_tar" mocap="true" pos="0 0 0"><!-- BODY 1 -->
			<geom type="sphere" size="0.003" rgba="1 0 0 0"/><!-- made transparent for now -->
		</body>
		<body name="s_pft" pos="0 0 0"><!-- Foot --><!-- BODY 2 -->
			<inertial pos="0 0. 0" mass="pft_mass" diaginertia="pft_MOI_XX pft_MOI_YY pft_MOI_ZZ" />
			<geom name="geom_pft" type="capsule" fromto="0 0 0 0 0 pft_length" size="pft_radius" rgba="0.5 0.5 0 .4"/>
			<joint name="j_pft" type="free" pos="0 0 0" stiffness="0" damping="0" limited="false"/>
			<site name="t_ank_dst" pos="t_ank_dst_pos_X 0 t_ank_dst_pos_Z" size="0.001" rgba="0 0 1 1"/>
			<body name="s_tar" pos="0 0 tar_pos"><!-- Tarsals --><!-- BODY 3 -->
				<inertial pos="0 0. 0" mass="tar_mass" diaginertia="tar_MOI_XX tar_MOI_YY tar_MOI_ZZ" /><!-- mass + moi of foot combined with tarsus-->
				<geom name="geom_tar" type="capsule" fromto="0 0 0 0 0 tar_length" size="tar_radius" rgba="0.5 0.5 0 .4"/>
				<joint name="j_tmt" type="hinge" axis="0 1 0" pos="0 0 0" solreflimit="0.002 1" solimplimit="0.9 0.9 0.001"/>
				<!-- <site name="t_ank_prx" pos="-0.002 0 0.004" size="0.001" rgba="0 0 1 1"/> -->
				<geom name="jointSphere_tmt" type="sphere" pos="0 0 0" size="jointSphere_tmt_radius" rgba=".3 .9 .3 .4"/><!-- seems to be necessary for ground contact -->
				<site name="t_tmt_side" pos="t_tmt_side_pos_X 0 0" size="0.0003" rgba="0 1 1 1"/>
				<site name="t_ank_via" pos="t_ank_via_pos_X 0 t_ank_via_pos_Z" size="0.001" rgba="1 0 0 1"/>
				<body name="s_tib" pos="0 0 sha_pos"><!-- Tibfib --><!-- BODY 4 -->
					<inertial pos="0 0 0" mass="sha_mass" diaginertia="sha_MOI_XX sha_MOI_YY sha_MOI_ZZ"/>
					<geom name="geom_sha" type="capsule" fromto="0 0 0 0 0 sha_length" size="sha_radius" rgba="0.5 0.5 0 .4"/>
					<joint name="j_ank" type="hinge" axis="0 1 0" pos="0 0 0"/> <!-- <springdamper=".002 1" solreflimit="0.002 0.5" solimplimit="0.9 0.9 0.001"--> 
					<site name="t_ank_prx" pos="t_ank_prx_pos_X 0 t_ank_prx_pos_Z" size="0.001" rgba="0 0 1 1"/>
					<site name="t_kne_dst" pos="0.002 0 0.004" size="0.001" rgba="0 0 1 1"/>
					<geom name="jointSphere_ank" type="sphere" pos="0 0 0" size="jointSphere_ank_radius" rgba=".3 .9 .3 .4"/>
					<site name="t_ank_side" pos="t_ank_side_pos_X 0 0" size="0.0003" rgba="0 1 1 1"/>
					<body name="s_fem" pos="0 0 thg_pos"><!-- Femur --><!-- BODY 5 -->
						<inertial pos="0 0 0" mass="thg_mass" diaginertia="thg_MOI_XX thg_MOI_YY thg_MOI_ZZ"/>
						<geom name="geom_thg" type="capsule" fromto="0 0 0 0 0 thg_length" size="thg_radius" rgba="0.7 0.7 0 .4"/>
						<joint name="j_kne" type="hinge" axis="0 1 0" pos="0 0 0"/>
						<site name="t_kne_prx" pos="0.002 0 0.004" size="0.001" rgba="0 0 1 1"/>
						<site name="t_hip_dst" pos="-0.002 0 0.004" size="0.001" rgba="0 0 1 1"/>
						<geom name="jointSphere_kne" type="sphere" pos="0 0 0" size="jointSphere_kne_radius" rgba="0 0 1 .5"/>
						<site name="t_kne_side" pos="0.003 0 0" size="0.0003" rgba="0 1 1 1"/>
						<body name="s_com" pos="0 0 com_pos_Z"><!-- BODY 6 for COM applying force -->
							<inertial pos="0 0 0" mass="0.000000000001"/>
							<geom type="sphere" pos="0 0 0" size="0.003" rgba="1 0 0 .4"/>
						</body>
						<body name="s_bod" pos="0 0 bod_pos"><!-- BODY 7 -->
							<inertial pos="0 0. 0" mass="bod_mass" diaginertia="bod_MOI_XX bod_MOI_YY bod_MOI_ZZ"/>
							<geom name="geom_bod" type="capsule" fromto="0 0 0 0 0 bod_length" size="bod_radius" rgba="0.5 0.5 0 .4"/><!-- fromto="0 0. 0.046286  0 0. 0.0660299 -->
							<site name="t_hip_prx" pos="-0.002 0 0.004" size="0.001" rgba="0 0 1 1"/>
							<geom name="jointSphere_hip" type="sphere" pos="0 0. 0" size="jointSphere_hip_radius" rgba=".3 .9 .3 .4"/>
							<site name="t_hip_side" pos="-0.003 0 0" size="0.0003" rgba="0 1 1 1"/>
							<joint name="j_hip" type="hinge" axis="0 1 0" pos="0 0 0"/>
							<geom name="snoutSphere" type="sphere" pos="0 0 snoutSphere_pos_Z" size="0.003" rgba="0 0 1 0.4"/>
						</body>
					</body>	
				</body>	
			</body>
		</body>
	</worldbody>
	<contact>
		<pair geom1="floor" geom2="geom_pft"  friction="1 1 0.005 0.0001 0.0001"/>
		<pair geom1="floor" geom2="jointSphere_hip"/>
		<pair geom1="floor" geom2="jointSphere_kne"/>
		<pair geom1="floor" geom2="jointSphere_ank"/>
		<pair geom1="floor" geom2="jointSphere_tmt"/>
	</contact>
	<equality>
 		<!-- <connect name="c_tar" body1="m_tar" body2="s_tar" anchor="0.003 0 0.0080277" active="false" /> -->
<!--  		<weld name="c_tar" body1="m_tar" body2="s_tar" active="false" /> -->
	</equality>
	<tendon>
		<spatial width="0.0002" rgba=".95 .3 .3 1" limited="false" name="tend_ank" stiffness="0">
			<site site="t_ank_prx"/>
			<geom geom="jointSphere_ank" sidesite="t_ank_side"/>
			<site site="t_ank_via"/>
			<geom geom="jointSphere_tmt" sidesite="t_tmt_side"/>
			<site site="t_ank_dst"/>
		</spatial>
<!-- 		<spatial width="0.0002" rgba=".95 .3 .3 1" limited="false" name="tend_kne">
			<site site="t_kne_prx"/>
			<geom geom="jointSphere_kne" sidesite="t_kne_side"/>
			<site site="t_kne_dst"/>
		</spatial>
		<spatial width="0.0002" rgba=".95 .3 .3 1" limited="false" name="tend_hip" stiffness="0">
			<site site="t_hip_prx"/>
			<geom geom="jointSphere_hip" sidesite="t_hip_side"/>
			<site site="t_hip_dst"/>
		</spatial> -->
	</tendon>
	<actuator>
	
		<!--  ================= Torque actuators ================= /-->
		<motor tendon='tend_ank' name='a_tend_ank'/>
		<!-- <motor tendon='tend_kne' name='a_tend_kne'/> -->
		<position kp="0.1" joint="j_kne" gear="1" ctrlrange="0 3.14" ctrllimited="false"/>
		<position kp="0.1" joint="j_hip" gear="1" ctrlrange="-3.14 0" ctrllimited="false"/>
		<!-- <motor tendon='tend_hip' name='a_tend_hip'/> -->
	</actuator>
	<sensor>
		<!-- <tendonpos tendon="tend_ank"/> -->
	</sensor>

	<keyframe>
		

		
 		 <!-- <key time="0" qpos="0.000	0.000	0.000	0.924	-0.000	-0.382	-0.000	1.570	-1.570	1.570"/>   -->

	</keyframe>
</mujoco>